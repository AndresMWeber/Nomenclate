

QCheckBox::indicator:checked{
    image: url(./nomenclate/ui/resource/icons/appbar.checkmark.thick.png);
}

QCheckBox::indicator:unchecked{
    image: url(./nomenclate/ui/resource/icons/appbar.checkmark.thick.unchecked.png);
}

QToolBar {
    background: red;
    spacing: 3px;
}

QToolBar::handle {
    image: url(handle.png);
}

QListWidget::item {
    border-style: solid;
    border-width: 1px;
    border-color: black;
    background: green;
}

QListWidget::item:selected {
    background: red;
}

QTableView {
    selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #656D78, stop: 1 #263238);
}

QTableView QTableCornerButton::section {
    background: red;
    border: 2px outset red;
}

QTreeView {
    show-decoration-selected: 1;
}

QStyledItemDelegate::item {
    margin: 2px;
}


QListView::item {
    background-color: red;
}

QListView::item:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #e7effd, stop: 1 #cbdaf1);
    border: 1px solid #bfcde4;
}

QListView::item:selected {
    border: 1px solid #567dbc;
}

QListView::item:selected:active{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6ea1f1, stop: 1 #567dbc);
}

QListView::item:selected:!active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6b9be8, stop: 1 #577fbf);
}